{
  "name": "invi.sible.link",
  "version": "1.0.0",
  "description": "This is https://invi.sible.link codebase",
  "main": "index.js",
  "scripts": {
    "build-css": "stylus styles/index.styl",
    "watch-css": "stylus styles/index.styl -o dist/css -w",
    "clean": "rm -rf dist/css dist/js",
    "build": "./.gruntlame",
    "start": "echo 'nop, call run storyteller or the others components separately' ",
    "storyteller": "nodemon --config config/storyteller.json bin/storyteller",
    "social-pressure": "nodemon --config config/social-pressure.json bin/social-pressure",
    "v2-social-pressure": "nodemon --config config/v2-social-pressure.json bin/v2-social-pressure",
    "v3-social-pressure": "nodemon --config config/v3-social-pressure.json bin/v3-social-pressure",
    "vigile": "nodemon --config config/vigile.json bin/vigile",
    "exposer": "DEBUG=*,-express:* node bin/exposer",
    "badger": "type=badger DEBUG=*,-lib:urlutils,-lib:cmdspawn node bin/chopsticks",
    "phantom": "type=basic DEBUG=*,-lib:urlutils,-lib:cmdspawn node bin/chopsticks"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vecna/invi.sible.link.git"
  },
  "keywords": [
    "tracking",
    "trackers",
    "javascript",
    "fingerprint"
  ],
  "author": "Claudio Agosti",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/vecna/invi.sible.link/issues"
  },
  "homepage": "https://github.com/vecna/invi.sible.link#readme",
  "dependencies": {
    "abp-filter-parser": "^0.2.0",
    "bluebird": "^3.4.6",
    "body-parser": "^1.18.3",
    "bootstrap": "4.0.0",
    "browser-sync": "^2.24.7",
    "c3": "^0.4.11",
    "crypto": "0.0.3",
    "d3": "^3.5.17",
    "d3-path": "^1.0.5",
    "d3-sankey": "^0.4.2",
    "datatables.net": "^1.10.12",
    "datatables.net-dt": "^1.10.12",
    "debug": "^2.6.8",
    "express": "^4.16.3",
    "gulp": "^4.0.0",
    "jquery": "^3.1.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "mongodb": "^2.2.11",
    "nconf": "^0.8.4",
    "node-geoip": "^1.0.1",
    "nodemon": "^1.18.4",
    "octicons": "^8.1.0",
    "phantomjs-prebuilt": "^2.1.16",
    "pug": "^2.0.0-beta6",
    "puppeteer": "^1.8.0",
    "request": "^2.88.0",
    "reveal.js": "^3.6.0",
    "rimraf": "^2.5.4",
    "stylus": "^0.54.5"
  },
  "devDependencies": {
    "eslint": "^3.12.0",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-import": "^2.14.0"
  }
}
